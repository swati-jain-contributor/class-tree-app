<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1001" id="com.classtree.android" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LearnerTalks</name>
    <description>
        Knowledge sharing platform
    </description>
    <author email="classtreecare@gmail.com" href="http://classtree.in">
        ClassTree Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AllowInlineMediaPlayback" value="true" />
</widget>
